<launch>
    <!-- Initialize -->
    <include file="$(find control)/launch/parts/pre.xml" />

    <!-- Load gio input configuration -->
    <rosparam
        file="$(find control)/config/gio_control.yaml"
        command="load"
    />

    <arg name="file" />
    <param
        name="datfile"
        type="str"
        value="$(eval find('control') + '/resources/paths/' + arg('file'))"
    />

    <node pkg="control" type="gio" name="gio" output="screen" />
</launch>
